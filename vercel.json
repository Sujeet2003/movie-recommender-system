{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",  
      "use": "@vercel/static-build",  
      "config": {
        "distDir": "staticfiles"
      }
    },
    {
      "src": "TMRH/wsgi.py", 
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "TMRH/wsgi.py"
    }
  ]
}